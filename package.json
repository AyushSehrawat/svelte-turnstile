{
	"name": "svelte-turnstile",
	"version": "0.5.0",
	"scripts": {
		"dev": "vite dev",
		"package": "svelte-kit sync && svelte-package",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.20.2",
		"@sveltejs/package": "^2.1.0",
		"ghostsui": "^1.6.0",
		"sass": "^1.63.6",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.4",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9"
	},
	"peerDependencies": {
		"svelte": "^3.58.0 || ^4.0.0"
	},
	"dependencies": {
		"turnstile-types": "^1.2.0"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ghostdevv/svelte-turnstile.git"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ghostdevv/svelte-turnstile/issues"
	},
	"author": "GHOST <ghostdevbusiness@gmail.com> (https://ghostdev.xyz)",
	"contributors": [
		{
			"name": "Braden Wiggins",
			"email": "braden@fracal-hq.com"
		}
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js"
}